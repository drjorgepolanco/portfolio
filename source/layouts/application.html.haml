!!!
%html
  %head
    %meta{charset: "utf-8"}
    %meta{content: "IE=edge,chrome=1", 
          http: {equiv: "X-UA-Compatible"}}
    %meta{name: "viewport", 
          content: "width=device-width, initial-scale=1"}
    %title= current_page.data.title || "Dr. Jorge Polanco"
    = stylesheet_link_tag "application"

  %body{class: page_classes}
    = yield
    = javascript_include_tag "//ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"
    = javascript_include_tag "//cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.min.js"
    = javascript_include_tag "vendor/wow"
    = javascript_include_tag "vendor/imagescroll"
    = javascript_include_tag "vendor/slick"
    = javascript_include_tag "vendor/particles"
    = javascript_include_tag "vendor/animated-headline"
    = javascript_include_tag "vendor/charcycle"
    = javascript_include_tag "libraries"
    = javascript_include_tag "scroll"
    = javascript_include_tag "main"
    = javascript_include_tag "imagescroll"